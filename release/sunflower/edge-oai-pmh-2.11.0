{
  "id" : "edge-oai-pmh-2.11.0",
  "name" : "OAI-PMH Edge API",
  "requires" : [ {
    "id" : "oai-pmh",
    "version" : "3.0"
  }, {
    "id" : "login",
    "version" : "5.0 6.0 7.0"
  } ],
  "provides" : [ ],
  "permissionSets" : [ ],
  "launchDescriptor" : {
    "dockerImage" : "folioorg/edge-oai-pmh:2.11.0",
    "dockerPull" : true,
    "env" : [ {
      "name" : "JAVA_OPTIONS",
      "value" : "-XX:MaxRAMPercentage=66.0 -Drequest_timeout_ms=7200000"
    } ],
    "dockerArgs" : {
      "HostConfig" : {
        "Memory" : 268435456,
        "PortBindings" : {
          "8081/tcp" : [ {
            "HostPort" : "%p"
          } ]
        }
      }
    }
  }
}