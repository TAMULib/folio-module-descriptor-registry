{
  "id" : "edge-dcb-1.3.0",
  "name" : "Edge DCB API",
  "requires" : [ {
    "id" : "login",
    "version" : "7.3"
  }, {
    "id" : "transactions",
    "version" : "1.1"
  } ],
  "provides" : [ ],
  "permissionSets" : [ ],
  "launchDescriptor" : {
    "dockerImage" : "folioorg/edge-dcb:1.3.0",
    "dockerPull" : true,
    "env" : [ {
      "name" : "JAVA_OPTIONS",
      "value" : "-XX:MaxRAMPercentage=66.0 -Drequest_timeout_ms=7200000"
    } ],
    "dockerArgs" : {
      "HostConfig" : {
        "Memory" : 268435456,
        "PortBindings" : {
          "8081/tcp" : [ {
            "HostPort" : "%p"
          } ]
        }
      }
    }
  }
}