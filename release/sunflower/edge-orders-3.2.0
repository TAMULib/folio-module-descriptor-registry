{
  "id" : "edge-orders-3.2.0",
  "name" : "Acquisitions - Orders Edge API",
  "requires" : [ {
    "id" : "gobi",
    "version" : "1.12"
  }, {
    "id" : "ebsconet",
    "version" : "2.0"
  }, {
    "id" : "login",
    "version" : "5.0 6.0 7.0 7.3"
  } ],
  "provides" : [ ],
  "permissionSets" : [ ],
  "launchDescriptor" : {
    "dockerImage" : "folioorg/edge-orders:3.2.0",
    "dockerPull" : true,
    "env" : [ {
      "name" : "JAVA_OPTIONS",
      "value" : "-XX:MaxRAMPercentage=66.0"
    } ],
    "dockerArgs" : {
      "HostConfig" : {
        "Memory" : 268435456,
        "PortBindings" : {
          "8081/tcp" : [ {
            "HostPort" : "%p"
          } ]
        }
      }
    }
  }
}