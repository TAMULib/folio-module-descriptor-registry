{
  "id" : "edge-patron-5.1.2",
  "name" : "Patron Services Edge API",
  "requires" : [ {
    "id" : "patron",
    "version" : "4.2 5.1"
  }, {
    "id" : "circulation",
    "version" : "9.5 10.0 11.0 12.0 13.0 14.0"
  }, {
    "id" : "login",
    "version" : "5.0 6.0 7.0"
  } ],
  "provides" : [ ],
  "permissionSets" : [ ],
  "launchDescriptor" : {
    "dockerImage" : "folioorg/edge-patron:5.1.2",
    "dockerPull" : true,
    "env" : [ {
      "name" : "JAVA_OPTIONS",
      "value" : "-XX:MaxRAMPercentage=66.0"
    } ],
    "dockerArgs" : {
      "HostConfig" : {
        "Memory" : 268435456,
        "PortBindings" : {
          "8081/tcp" : [ {
            "HostPort" : "%p"
          } ]
        }
      }
    }
  }
}