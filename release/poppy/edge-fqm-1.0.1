{
  "id" : "edge-fqm-1.0.1",
  "name" : "Edge FQM API",
  "requires" : [ {
    "id" : "login",
    "version" : "7.0"
  }, {
    "id" : "fqm-query",
    "version" : "1.0"
  }, {
    "id" : "entity-types",
    "version" : "1.0"
  } ],
  "permissionSets" : [ ],
  "launchDescriptor" : {
    "dockerImage" : "folioorg/edge-fqm:1.0.1",
    "dockerPull" : true,
    "env" : [ {
      "name" : "JAVA_OPTIONS",
      "value" : "-XX:MaxRAMPercentage=66.0"
    }, {
      "name" : "okapi_url",
      "value" : "http://okapi:9130"
    }, {
      "name" : "secure_store",
      "value" : "Ephemeral"
    }, {
      "name" : "secure_store_props",
      "value" : "src/main/resources/ephemeral.properties"
    } ],
    "dockerArgs" : {
      "HostConfig" : {
        "Memory" : 357913941,
        "PortBindings" : {
          "8081/tcp" : [ {
            "HostPort" : "%p"
          } ]
        }
      }
    }
  }
}
