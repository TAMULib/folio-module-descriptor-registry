{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "labels": {
      "workload.user.cattle.io/workloadselector": "deployment-folio-scratch-{name:}"
    },
    "name": "{name:}",
    "namespace": "[NAMESPACE]"
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "workload.user.cattle.io/workloadselector": "deployment-folio-scratch-{name:}"
      }
    },
    "strategy": {
      "type": "RollingUpdate"
    },
    "template": {
      "metadata": {
        "labels": {
          "workload.user.cattle.io/workloadselector": "deployment-folio-scratch-{name:}",
          "app": "{name:}"
        },
        "namespace": "[NAMESPACE]"
      },
      "spec": {
        "containers": [
          {
            "env": "[ENVIRONMENT]",
            "envFrom": "[ENV_FROM]",
            "image": "[DOCKER_IMAGE]",
            "imagePullPolicy": "Always",
            "livenessProbe": "[LIVENESS_PROBE]",
            "name": "{name:}",
            "ports": "[CONTAINER_PORTS]",
            "readinessProbe": "[READINESS_PROBE]",
            "resources": {
              "limits": {
                "cpu": "[CPU_RESOURCE]",
                "memory": "[MEMORY_RESOURCE]"
              }
            },
            "volumeMounts": "[VOLUME_MOUNTS]"
          }
        ],
        "volumes": "[VOLUMES]"
      }
    }
  }
}
