{
  "apiVersion": "apps/v1",
  "kind": "StatefulSet",
  "metadata": {
    "labels": {
      "workload.user.cattle.io/workloadselector": "apps.statefulSet-folio-scratch---{name:}"
    },
    "name": "{name:}"
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "workload.user.cattle.io/workloadselector": "apps.statefulSet-folio-scratch---{name:}"
      }
    },
    "serviceName": "{name:}",
    "template": {
      "metadata": {
        "labels": {
          "workload.user.cattle.io/workloadselector": "apps.statefulSet-folio-scratch---{name:}",
          "app": "{name:}"
        }
      },
      "spec": {
        "containers": [
          {
            "args": "[CONTAINER_ARGS]",
            "env": "[ENVIRONMENT]",
            "envFrom": "[ENV_FROM]",
            "image": "[CONTAINER_IMAGE]",
            "imagePullPolicy": "[CONTAINER_IMAGE_PULL_POLICY]",
            "imagePullSecrets": "[CONTAINER_IMAGE_PULL_SECRETS]",
            "livenessProbe": "[LIVENESS_PROBE]",
            "name": "{name:}",
            "ports": "[CONTAINER_PORTS]",
            "readinessProbe": "[READINESS_PROBE]",
            "resources": "[RESOURCES]"
          }
        ],
        "volumes": "[VOLUMES]"
      }
    },
    "updateStrategy": {
      "type": "RollingUpdate"
    }
  }
}
