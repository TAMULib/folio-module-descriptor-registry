{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "app": "{global:app}",
      "app.kubernetes.io/name": "{name:}",
      "app.kubernetes.io/instance": "{id:}",
      "app.kubernetes.io/part-of": "FOLIO",
      "app.kubernetes.io/version": "{version:}",
      "environment": "scratch"
    },
    "name": "{name:}"
  },
  "spec": {
    "ipFamilies": [
      "IPv4"
    ],
    "ipFamilyPolicy": "SingleStack",
    "ports": [
      {
        "name": "primary-{name:}",
        "port": "[CONTAINER_PORT_PUBLIC]",
        "protocol": "TCP",
        "targetPort": "[CONTAINER_PORT]"
      }
    ],
    "selector": {
      "app": "{global:app}",
      "app.kubernetes.io/name": "{name:}",
      "app.kubernetes.io/instance": "{id:}",
      "app.kubernetes.io/part-of": "FOLIO",
      "app.kubernetes.io/version": "{version:}",
      "environment": "scratch"
    },
    "type": "ClusterIP"
  }
}
