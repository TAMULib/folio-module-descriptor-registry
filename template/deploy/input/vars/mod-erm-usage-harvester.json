{
  "[CONTAINER_PORT]": 8081,
  "[ENV_FROM]": [
    "[ENV_FROM_DB_CONNECT]",
    "[ENV_FROM_DB_CONNECT_SECRET]",
    "[ENV_FROM_GLOBAL]",
    "[ENV_FROM_KONG_CONNECT]"
  ],
  "[ENVIRONMENT]": [
    "[JAVA_OPTIONS]",
    "[MODULE_NAME]",
    "[MODULE_VERSION]",
    {
      "name": "CONFIG",
      "value": "{\"okapiUrl\": \"http://kong.{global:namespace}.svc:8000\"}"
    },
    {
      "name": "HAZELCAST_IP",
      "value": "$(MOD_ERM_USAGE_HARVESTER_SERVICE_HOST)"
    },
    {
      "name": "HAZELCAST_PORT",
      "value": "5701"
    }
  ],
  "[PORTS]": [
    {
      "containerPort": "[CONTAINER_PORT]",
      "name": "primary",
      "protocol": "TCP"
    }
  ]
}
