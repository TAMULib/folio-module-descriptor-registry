{
  "[CONTAINER_PORT]": 8080,
  "[ENV_FROM]": [
    "[ENV_FROM_DB_CONNECT]",
    "[ENV_FROM_DB_CONNECT_SECRET]",
    "[ENV_FROM_GLOBAL]"
  ],
  "[ENVIRONMENT]": [
    {
      "name": "GRAILS_SERVER_HOST",
      "value": "{name:}"
    },
    {
      "name": "GRAILS_SERVER_PORT",
      "value": "8080"
    },
    {
      "name": "JAVA_OPTIONS",
      "value": "-server -XX:+UseContainerSupport -XX:MaxRAMPercentage=50.0 -XX:+PrintFlagsFinal -Dlog4j2.formatMsgNoLookups=true"
    }
  ],
  "[JAVA_OPTIONS]": "-server -XX:+UseContainerSupport -XX:MaxRAMPercentage=50.0 -XX:+PrintFlagsFinal -Dlog4j2.formatMsgNoLookups=true"
}
