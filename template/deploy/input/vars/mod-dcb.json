    {
      "name": "KAFKA_SSL_KEYSTORE_PASSWORD",
      "value": ""
    },
    {
      "name": "KAFKA_SSL_TRUSTSTORE_LOCATION",
      "value": ""
    },
    {
      "name": "KAFKA_SSL_TRUSTSTORE_PASSWORD",
      "value": ""
    }
  ],
  "[JAVA_OPTIONS_VALUE]": "-XX:MaxRAMPercentage=80.0 -Dlog4j2.formatMsgNoLookups=true -Dserver.port=8080 -Dlog4j2.formatMsgNoLookups=true"
}
