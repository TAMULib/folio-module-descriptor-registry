{
  "[ENV_FROM]": [
    "[ENV_FROM_DB_CONNECT]",
    "[ENV_FROM_DB_CONNECT_SECRET]",
    "[ENV_FROM_GLOBAL]",
    "[ENV_FROM_KAFKA_CONNECT]"
  ],
  "[ENVIRONMENT]": [
    "[JAVA_OPTIONS]",
    {
      "name": "data.import.storage.type",
      "value": "LOCAL_STORAGE"
    },
    {
      "name": "data.import.storage.path",
      "value": "/storage/upload"
    },
    {
      "name": "file.processing.buffer.chunk.size",
      "value": "10"
    }
  ],
  "[JAVA_OPTIONS_VALUE]": "-XX:MaxRAMPercentage=80.0 -Djava.util.logging.config.file=vertx-default-jul-logging.properties"
}
